{
  "persist": true,
  "app": "Goldlabel",
  "description": "JavaScript whatever the weather",
  "url": "https://goldlabel.pro",
  "repo": "https://github.com/javascript-pro/core",
  "images": {
    "favicon": "/png/favicon.png"
  },
  "imagesArr": ["/png/favicon.png", "/png/og.png"],
  "themes": {
    "light": {
      "mode": "light",
      "primary": "#1db2d3",
      "secondary": "#408ef7",
      "background": "#FFF",
      "border": "#9FBCD1",
      "paper": "#F0F3F6",
      "text": "#303030"
    },
    "dark": {
      "mode": "dark",
      "primary": "#1db2d3",
      "secondary": "#3e8df7",
      "background": "#001321",
      "paper": "#0A2D47",
      "text": "#FFFFFF",
      "border": "#021929"
    }
  },
  "nav": [
    {
      "label": "PDFs",
      "icon": "pdf",
      "route": "/database/table/pdfs"
    },
    {
      "label": "All Tables",
      "icon": "database",
      "route": "/database"
    },
    {
      "label": "AKI",
      "icon": "ki",
      "route": "/ki"
    },
    {
      "label": "Logs",
      "icon": "info",
      "route": "/database/table/log"
    }
  ]
}
